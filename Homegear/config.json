{
  "name": "Homegear",
  "version": "0.21.9",
  "slug": "rpi-homegear",
  "description": "Homegear as a Hassio add-on",
  "startup": "before",
  "boot": "auto",
  "gpio": true,
  "apparmor": false,
  "fullaccess": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "arch": [
    "amd64",
    "armhf",
    "i386"
  ],
  "map": ["config:rw", "share:rw"],
  "ports": {
    "2001/tcp": 2001    
  },
  "environment": {
  },
  "options": {
  },
  "schema": {
  },
  "devices": [
    "/dev/ttyAMA0:/dev/ttyAMA0:rwm",
    "/dev/spidev0.0:/dev/spidev0.0:rwm"
  ]
}
